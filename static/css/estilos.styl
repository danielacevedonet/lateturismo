.Sidebar
  display inline-block
  right 0

.Sidebar-container
  border-radius 3px
  background white
  box-shadow 0 1px 2px rgba(0,0,0,0.15)
  display block
  margin-bottom 1.5em
  padding .9em

.Sidebar-imageBanner,
.Sidebar-containerImage
  margin 0
  padding 0

.Sidebar-title
  border-radius 3px
  display block
  color white
  font-weight bold
  font-size 1em
  margin-bottom 1.5em
  padding .5em .7em

.Sidebar-containerTitle
  color #2c2b2b
  font-size 1.3em
  font-weight normal
  margin 0

.Sidebar-containerList
  border-bottom 1px solid #e4e4e4
